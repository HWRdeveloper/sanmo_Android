        android:textColor="#707070"
        android:gravity="center"
        tools:layout_editor_absoluteX="54dp"
        tools:layout_editor_absoluteY="276dp" />

    <EditText
        android:id="@+id/pw_input"
        android:layout_width="58dp"
        android:layout_height="22dp"
